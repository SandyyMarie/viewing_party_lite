<p><%= button_to "Top Rated Movies", '/users/:user_id/movies?q=top%20rated' %></p>

<h1>OR</h1>

<%= form_with url: '/users/:user_id/movies?q=keyword', method: :get, local: true do |form| %>
    <p><%= form.label :search, "Search by movie title"%></p>
    <%= form.text_field :search%>

    <p><%= form.submit "Find Movies" %></p>
<% end %>